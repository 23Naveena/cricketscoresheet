//sub subprogram
#include"welcome.h"
#include"menu.h"
#include"newscoresheet"
#include"declaration"
void menu()
{
    printf("\n\n\t\t\t\tMenu:\n");
    printf("\n\n\n1.New Scoresheet:\n");
    printf("\n\n\n2.View Scoresheet:\n");
    printf("\n\n\n3.Exit\n");
}
void date();
void print();
void filewrite();
void fileread();
void fileopen(char);
void limitinput();
//main program
int main()
{
    system("color f1");
    int a,key;
    char b;
    File*fnew;
    char lastinput;
    while(TRUE)
    {
        welcome();
        system("cls");
        menu();
        ch=getch();
        char ch;
        switch(ch)
        {
            case '1' :
            fileopen(ch);
            case '2':
            fileopen(ch);
            getch();
            case '3':
            system("cls");
            exit(0);
        }
    }
}
         
     
